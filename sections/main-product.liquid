                  <div class="label">{{ block.settings.title | escape }}</div>
                  <div class="fixed-layout">
                    {%- liquid
                      assign resource_list_type = ''
                      assign resource_list_handle = ''
                      for i in (1..2)
                        assign setting_key = 'link_' | append: i
                        if block.settings[setting_key] != blank
                          assign url_split = block.settings[setting_key] | split: '?' | first | split: '/'
                          assign resource_handle = url_split | last
                          assign resource_type_index = url_split | size | minus: 2
                          assign resource_type_index_minus_1 = resource_type_index | minus: 1
                          if url_split.size == 4 and url_split[resource_type_index_minus_1] == 'blogs'
                            assign resource_type = 'articles'
                            assign resource_handle = url_split[resource_type_index] | append: '/' | append: resource_handle
                          else
                            assign resource_type = url_split[resource_type_index]
                          endif

                          if resource_list_type != blank
                            assign resource_list_type = resource_list_type | append: ','
                            assign resource_list_handle = resource_list_handle | append: ','
                          endif
                          assign resource_list_type = resource_list_type | append: resource_type
                          assign resource_list_handle = resource_list_handle | append: resource_handle
                        endif
                      endfor

                      assign resource_list_type_arr = resource_list_type | split: ','
                      assign resource_list_handle_arr = resource_list_handle | split: ','

                      if settings.prod_thumb_shape == 'portrait-23'
                        assign chosen_aspect_ratio_product = 0.67
                      elsif settings.prod_thumb_shape == 'portrait-45'
                        assign chosen_aspect_ratio_product = 0.8
                      elsif settings.prod_thumb_shape == 'square'
                        assign chosen_aspect_ratio_product = 1.0
                      elsif settings.prod_thumb_shape == 'landscape-54'
                        assign chosen_aspect_ratio_product = 1.25
                      elsif settings.prod_thumb_shape == 'landscape-32'
                        assign chosen_aspect_ratio_product = 1.50
                      elsif settings.prod_thumb_shape == 'tallest' and resource_list_type == 'products,products'
                        assign chosen_aspect_ratio_product = all_products[resource_list_handle_arr.first].featured_media.preview_image.aspect_ratio | at_most: all_products[resource_list_handle_arr.last].featured_media.preview_image.aspect_ratio
                      elsif settings.prod_thumb_shape == 'shortest' and resource_list_type == 'products,products'
                        assign chosen_aspect_ratio_product = all_products[resource_list_handle_arr.first].featured_media.preview_image.aspect_ratio | at_least: all_products[resource_list_handle_arr.last].featured_media.preview_image.aspect_ratio
                      else
                        assign chosen_aspect_ratio_product = false
                      endif

                      if settings.coll_grid_image_shape == 'portrait-23'
                        assign chosen_aspect_ratio_collection = 0.67
                      elsif settings.coll_grid_image_shape == 'portrait-45'
                        assign chosen_aspect_ratio_collection = 0.8
                      elsif settings.coll_grid_image_shape == 'square'
                        assign chosen_aspect_ratio_collection = 1.0
                      elsif settings.coll_grid_image_shape == 'landscape-54'
                        assign chosen_aspect_ratio_collection = 1.25
                      elsif settings.coll_grid_image_shape == 'landscape-32'
                        assign chosen_aspect_ratio_collection = 1.50
                      elsif settings.coll_grid_image_shape == 'tallest' and resource_list_type == 'collections,collections'
                        assign chosen_aspect_ratio_collection = collections[resource_list_handle_arr.first].featured_image.aspect_ratio | at_most: collections[resource_list_handle_arr.last].featured_image.aspect_ratio
                      elsif settings.coll_grid_image_shape == 'shortest' and resource_list_type == 'collections,collections'
                        assign chosen_aspect_ratio_collection = collections[resource_list_handle_arr.first].featured_image.aspect_ratio | at_least: collections[resource_list_handle_arr.last].featured_image.aspect_ratio
                      else
                        assign chosen_aspect_ratio_collection = false
                      endif

                      for type in resource_list_type_arr
                        assign handle = resource_list_handle_arr[forloop.index0]
                        echo '<div class="column column--half">'
                        case type
                          when 'products'
                            render 'product-block', product: all_products[handle], no_quick_buy: true, custom_aspect_ratio: chosen_aspect_ratio_product
                          when 'collections'
                            render 'collection-block', collection: collections[handle], custom_aspect_ratio: chosen_aspect_ratio_collection
                          when 'articles'
                            render 'article-block-small', article: articles[handle]
                        endcase
                        echo '</div>'
                      endfor
                    -%}
                  </div>
                </div>
              {% endif %}
          {% endcase %}
        {% endfor %}

        <div class="lightly-spaced-row-above only-in-quickbuy">
          <a class="more" href="{{ product.url }}">
            <span class="beside-svg underline">{{ 'products.quick_buy.read_more' | t }}</span>
            <span class="icon--small icon-natcol">{% render 'svg-chevron-right' %}</span>
          </a>
        </div>
      </div>

      <script type="application/json" id="ProductJson-{{ product.id }}">
        {{- product | json -}}
