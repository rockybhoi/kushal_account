<script>
 var vAudio = document.getElementById("myAudio");
    document.addEventListener("visibilitychange", handleVisibilityChange, false);
   function handleVisibilityChange() {
    if (document.hidden) {
      vAudio.pause();
    } 
    else 
    {
      if (jQuery(".ct_mute_numute").hasClass("ct_play")) {
           vAudio.play();
       }else
       {
         vAudio.pause();
       }
    }
  }
   function playAudio()
   {
     if (vAudio.paused) {
         vAudio.play();
       jQuery(".ct_mute_numute").addClass("ct_play")
        jQuery(".ct_mute_numute").removeClass("ct_mute")
     }else
     {
       jQuery(".ct_mute_numute").addClass("ct_mute")
       jQuery(".ct_mute_numute").removeClass("ct_play")
       
       vAudio.pause();
     }
          
    }

  
</script>





