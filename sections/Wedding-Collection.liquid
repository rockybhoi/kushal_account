    a.ctreview-box_link {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
}
  span.Velen_title {
    color: #CF2228;
    font-family: 'Marcellus';
    font-size: 40px;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
}
  .velen_active {
    padding-top: 47px;
}
     .velen_active  .assassas {
    background: inherit;
}
  .velen_active .wead-content-head {
    font-family: 'Lato' !important;
    max-width: 200px;
}
  .velen_active  .wead-content-text {
    height: 65px;
}

  .sub_text_active {
    padding-bottom: 5px;
}
  .Wedding-Collection-head_subtext {
    font-size: 24px;
    font-family: 'Inter', sans-serif;
    font-style: normal;
    font-weight: 400;
    line-height: normal;
    padding-bottom: 40px;
    text-align: left;
    color: #CF2228;
}

  @media (max-width: 600px) {
    .Wedding-Collection-head_subtext {
    font-size: 18px;
    }
 .velen_active .ctreview-box {
    max-height: 400px;
  }
    .velen_active  .wead-content-text {
    height: 47px;
}
    .velen_active .wead-content-head {
    max-width: 150px;
}
 .velen_active .main_coll_img {
    height: 400px;
    border-radius: 10px !important;
    object-fit: cover;
}
   .velen_active  .assassas {
    background: inherit;
}
}
</style>

<div class="Wedding-Collection {% if section.settings.velen_active %}velen_active {% endif %} {% if section.settings.style_sec %} Wedding-Collection_style_sec  {% endif %} ">
  <div class="Wedding-Collection-head {% if section.settings.sbtext !=blank %} sub_text_active  {% endif %} ">{{ section.settings.head }}{% if section.settings.sec_tit !=blank %}<span class="Velen_title"> {{ section.settings.sec_tit }}</span>{% endif %}</div>
  {% if section.settings.sbtext !=blank %}
      <div class="Wedding-Collection-head_subtext">{{ section.settings.sbtext }}</div>
  {% endif %}
  <div class="Wedding-Collection-inner items_scroll">
    {% for block in section.blocks %}
      <div class="Wedding-Collection-box ctreview-box">
                <a href="{{ block.settings.link }}" class="ctreview-box_link"></a>
        <img
          loading="lazy"
          src="{{ block.settings.image | image_url: width: 400 }}"
          data-src="{{ block.settings.image | image_url: width: 400 }}"
          width="{{ block.settings.image.width }}"
          height="{{ block.settings.image.height }}"
          alt="{{ block.settings.image.alt }}"
          class=" main_coll_img"
        >
        <div class="wead-content">
         {% if section.settings.style_sec %} <div class="assassas"></div> {% endif %}
          <div class="wead-content-head ">{{ block.settings.heading }}</div>
          <div class="wead-content-text">{{ block.settings.text }}</div>

          {% if block.settings.link != blank %}
            <a href="{{ block.settings.link }}">
              {{ block.settings.link_text }}
              {% comment %}
              <img
                loading="lazy"
                src="https://cdn.shopify.com/s/files/1/0796/1959/files/Line_arrow-right.png?v=1700636517"
                data-src="https://cdn.shopify.com/s/files/1/0796/1959/files/Line_arrow-right.png?v=1700636517"
                width="18"
                height="18"
                alt=""
                class=" link_areo"
              >
              {% endcomment %}
            
            </a>
          {% endif %}
        </div>
      </div>
    {% endfor %}
  </div>
</div>

{% schema %}
{
  "name": "Wedding-Collection",
  "settings": [
    {
  "type": "checkbox",
  "id": "velen_active",
  "label": "Valentine Page",
  "default": false
},

    {
          "type": "text",
          "id": "head",
          "label": "Heading"
      },
    {
          "type": "text",
          "id": "sec_tit",
          "label": "Secend Heading"
      },
    {
          "type": "text",
          "id": "sbtext",
          "label": "Sub-Text"
      },
    {
        "type": "checkbox",
        "id": "style_sec",
        "label": "Secend Style",
        "default": false
      }

  ],
  "blocks":[{
    "type":"Box",
    "name":"Collection Box",
    "settings":[
      {
        "type": "image_picker",
        "id": "image",
        "label": "Collection Image"
      },
      {
          "type": "text",
          "id": "heading",
          "label": "Heading"
      },
      {
          "type": "text",
          "id": "text",
          "label": "Sub-text"
      },
      {
        "type": "url",
        "id": "link",
        "label": "Button link"
      },
      {
          "type": "text",
          "id": "link_text",
          "label": "Link Text",
          "default":"Explore"
      }



    ]
  }],
  "presets":[{
        "name": "Wedding-Collection"
  }]
}
{% endschema %}

