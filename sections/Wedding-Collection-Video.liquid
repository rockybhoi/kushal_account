{{ 'swiper-bundle.min.css' | asset_url | stylesheet_tag: preload: true }}
   <script  src="{{ 'swiper-bundle.min.js'  | asset_url }}" type="text/javascript" ></script>



<div class="Wedding-Collection  {% if section.settings.mother %} motherday-Collection {% endif %} {% if section.settings.velen_active %}velen_active_sec {% endif %} {% if section.settings.style_sec %} Wedding-Collection_style_sec  {% endif %} ">
  <div class="Wedding-Collection-head {% if section.settings.head_link != blank %} Wedding-Collection-head_link {% endif %} {% if section.settings.sbtext !=blank %} sub_text_active  {% endif %}">{{ section.settings.head }} {% if section.settings.sec_tit !=blank %}<span class="Velen_title"> {{ section.settings.sec_tit }}</span>{% endif %} {% if section.settings.head_link != blank %}<a class="head_title_link" href="{{ section.settings.head_link }}">View All</a>{% endif %}</div>
   {% if section.settings.sbtext !=blank %}
      <div class="Wedding-Collection-head_subtext">{{ section.settings.sbtext }}</div>
  {% endif %}

  <div class="swiper  swiper_Video">
  <div class="Wedding-Collection-inner_vide swiper-wrapper ">
    {% for block in section.blocks %}
      <div class="swiper-slide Wedding-Collection-box_vid ct_hover_play">
                <a href="{{ block.settings.link }}" class="ctreview-box_link"></a>

      
 <video    loop muted playsinline class="uvodna__bg-video">
    <source src="{{ block.settings.video_link }}" type="video/mp4" />
</video>



        

        {% if block.settings.image !=blank %}
        <img
          loading="lazy"
          src="{{ block.settings.image | image_url: width: 400 }}"
          data-src="{{ block.settings.image | image_url: width: 400 }}"
          width="{{ block.settings.image.width }}"
          height="{{ block.settings.image.height }}"
          alt="{{ block.settings.image.alt }}"
          class=" main_coll_img main_coll_img_sec"
        >
        {% endif %}
        <div class="wead-content">
         {% if section.settings.style_sec %} <div class="assassas"></div> {% endif %}
          <div class="wead-content-head">{{ block.settings.heading }}</div>
          
         {% if block.settings.text !=blank %} <div class="wead-content-text">{{ block.settings.text }}</div>{% endif %}

          {% if block.settings.link != blank %}
            <a href="{{ block.settings.link }}">
              {{ block.settings.link_text }}
              <!-- <img
                loading="lazy"
                src="https://cdn.shopify.com/s/files/1/0796/1959/files/Line_arrow-right.png?v=1700636517"
                data-src="https://cdn.shopify.com/s/files/1/0796/1959/files/Line_arrow-right.png?v=1700636517"
                width="18"
                height="18"
                alt=""
                class=" link_areo"
              > -->
            </a>
          {% endif %}
        </div>


    


        
      </div>
    {% endfor %}
  </div>

      <!-- <div class="swiper-pagination CT_swiper-pagination"></div> -->
    
  </div>   
</div>



