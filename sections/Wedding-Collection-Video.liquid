{% schema %}
{
  "name": "Wedding-Collection",
  "settings": [
     {
  "type": "checkbox",
  "id": "velen_active",
  "label": "Valentine Page",
  "default": false
},

{
  "type": "checkbox",
  "id": "mother",
  "label": "mother day Page",
  "default": false
},
    {
          "type": "text",
          "id": "head",
          "label": "Heading"
      },
    {
          "type": "text",
          "id": "sec_tit",
          "label": "Secend Heading"
      },
     {
          "type": "text",
          "id": "sbtext",
          "label": "Sub-Text"
      },
         {
          "type": "url",
          "id": "head_link",
          "label": "View All Button Link"
      },
    {
        "type": "checkbox",
        "id": "style_sec",
        "label": "Secend Style",
        "default": false
      }

  ],
  "blocks":[{
    "type":"Box",
    "name":"Collection Box",
    "settings":[
      {
        "type": "text",
        "id": "video_link",
        "label": "Video Link",
        "info":"Upload Video in Media Content And Copy Link Ad Paste"
      },
      {
        "type": "image_picker",
        "id": "image",
        "label": "Collection Image"
      },
      {
          "type": "text",
          "id": "heading",
          "label": "Heading"
      },
      {
          "type": "text",
          "id": "text",
          "label": "Sub-text"
      },
      {
        "type": "url",
        "id": "link",
        "label": "Button link"
      },
      {
          "type": "text",
          "id": "link_text",
          "label": "Link Text",
          "default":"Explore"
      }



    ]
  }],
  "presets":[{
        "name": "Wedding-Collection-Video"
  }]
}
{% endschema %}

