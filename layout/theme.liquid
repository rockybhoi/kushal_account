    <script>
      
      
  const io = new IntersectionObserver((entries) =>
    entries.forEach((entry) => {
      if (entry.isIntersecting) {
        const image = entry.target;
        if (image.dataset.src.indexOf('width') > 0 || image.dataset.src.indexOf('height') > 0) {
          baseImgUrl = image.dataset.src.replace(/&width=[0-9]/, '').replace(/&height=[0-9]/, '');
          image.src = `${baseImgUrl}&width=${image.clientWidth}`;
        } else {
          image.src = `${image.dataset.src}&width=${image.clientWidth}`;
        }
        image.classList.add('img-loaded');
        io.disconnect(image);
      }
    })
    );
    window.addEventListener('DOMContentLoaded', () => {
      document.querySelectorAll(".lazyImage").forEach((element) => io.observe(element));
    })


    </script>
